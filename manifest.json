{
    "manifest_version": 2,
    "name": "RealEstate Price Viewer",
    "version": "1.0",
    "description": "View real prices of houses on realestate.com.au",
    "permissions": [
      "activeTab",
      "storage",
      "webNavigation"
    ],
    "browser_action": {
      "default_icon": {
        "48": "icon.png"
      },
      "default_title": "RealEstate Price Viewer"
    },
    "content_scripts": [
      {
        "matches": ["*://www.realestate.com.au/property/*"],
        "js": ["content/content.js"]
      }
    ],
    "icons": {
      "48": "images/icon.jpg"
    }
  }
  